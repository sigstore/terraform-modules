                  "prometheusQuery": "label_replace(\n  sum by (version) (increase({__name__=\"logging.googleapis.com/user/tuf/client_user_agents\", client=\"sigstoreJavaClient\"}[7d])),\n  \"version\",\n  \"unknown\",\n  \"version\",\n  \"^$\"\n)"
                }
